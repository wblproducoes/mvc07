{
  "name": "wblproducoes/mvc07",
  "description": "Sistema Administrativo PHP MVC com Twig 3.0",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.4.0",
    "twig/twig": "^3.0",
    "phpmailer/phpmailer": "^6.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
